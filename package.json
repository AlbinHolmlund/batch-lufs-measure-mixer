{
  "name": "batch-lufs-measure-mixer",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://skrap.info/audio",
  "main": "public/electron.js",
  "dependencies": {
    "@domchristie/needles": "^0.0.2-1",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/material": "^5.11.13",
    "@mui/styled-engine-sc": "^5.11.11",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "audiobuffer-to-wav": "^1.0.0",
    "dotenv": "^16.0.3",
    "electron-is-dev": "^2.0.0",
    "express": "^4.18.2",
    "file-saver": "^2.0.5",
    "get-port-sync": "^1.0.1",
    "jszip": "^3.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flags": "^0.1.18",
    "react-nbsp": "^1.2.4",
    "react-scripts": "5.0.1",
    "styled-components": "^5.3.9",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-start": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-build": "npm run build && electron-builder",
    "release": "npm run electron-build -- -p always"
  },
  "build": {
    "appId": "info.skrap.batchlufsmeasuremixer",
    "productName": "Batch LUFS Measure & Mixer Tool",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "extraResources": [
      "build"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "electron": "^23.1.4",
    "electron-builder": "^23.6.0",
    "wait-on": "^7.0.1"
  }
}